{
  "name": "tps-lookup",
  "version": "1.0.0",
  "description": "Lookup TPS numbers",
  "main": "app.js",
  "scripts": {
    "compileAll": "./bin/main.sh compileAll",
    "postinstall": "npm run compileAll",
    "start": "node app.js",
    "start:dev": "doppler run -p ft-tps-screener -c dev -- node app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update-numbers": "doppler run -p ft-tps-screener -c dev -- node scripts/update-numbers.js",
    "update": "doppler run -p ft-tps-screener -c prod -- node scripts/trim-phone-numbers.js",
    "trim-numbers-in-dbs": "doppler run -p ft-tps-screener -c prod -- node scripts/trim-phone-numbers.js"
  },
  "author": "Andrew Snead",
  "license": "ISC",
