    "@dotcom-reliability-kit/logger": "^3.1.1",
    "@financial-times/health-check": "^2.1.9",
    "@financial-times/okta-express-middleware": "1.1.0",
    "aws-sdk": "^2.822.0",
    "body-parser": "^1.17.2",
    "browserify": "^17.0.0",
    "co": "^4.6.0",
    "co-wait": "0.0.0",
    "compression": "^1.6.2",
    "cookie-session": "^1.4.0",
    "dynamo-backup-to-s3": "^0.6.1",
    "event-stream": "^3.3.4",
    "express": "^4.19.2 ",
    "express-session": "^1.18.1",
    "form-data": "^2.5.1",
    "sass": "^1.49.9",
    "ssh2": "^1.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "overrides": {},
  "volta": {
    "node": "20.19.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.1.1"
  }
