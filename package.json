{
  "name": "tps-lookup",
  "version": "1.0.0",
  "description": "Lookup TPS numbers",
  "main": "app.js",
  "scripts": {
    "compileAll": "./bin/main.sh compileAll",
    "postinstall": "npm run compileAll",
    "prettier": "prettier --write .",
    "start": "node app.js",
    "start:dev": "doppler run -p ft-tps-screener -c dev -- node app.js",
    "test": "npm run -s test:lint && npm run -s test:coverage",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.js'",
    "test:lint": "eslint --quiet . && prettier --check . && echo \"\nðŸŒ» Linting done\"",
    "test:watch": "npm run test:coverage -- --watch",
    "update": "node scripts/update-numbers.js",
    "update-numbers": "doppler run -p ft-tps-screener -c dev -- node scripts/update-numbers.js"
  },
