  "license": "ISC",
  "dependencies": {
    "@financial-times/health-check": "^2.1.9",
    "@financial-times/n-logger": "^6.2.0",
    "@financial-times/okta-express-middleware": "0.0.9",
    "aws-sdk": "^2.822.0",
    "body-parser": "^1.17.2",
    "browserify": "^14.5.0",
    "co": "^4.6.0",
    "co-wait": "0.0.0",
    "compression": "^1.6.2",
    "cookie-session": "^1.4.0",
    "dotenv": "^4.0.0",
    "dynamo-backup-to-s3": "^0.6.1",
    "eslint-plugin-jest": "^23.20.0",
    "event-stream": "^3.3.4",
