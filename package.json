{
  "name": "tps-lookup",
  "version": "1.0.0",
  "description": "Lookup TPS numbers",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "compileAll": "./bin/main.sh compileAll",
    "postinstall": "npm run compileAll",
    "start": "node app.js",
    "prepare": "npm run snyk-protect; snyk protect",
    "snyk-protect": "snyk protect",
    "vault:env": "vault read --format json /secret/teams/internal-products/ft-tps-screener/production | jq -r \".data|to_entries|map(\\\"\\(.key)='\\(.value|tostring)'\\\")|.[]\" > .env"
  },
  "author": "Andrew Snead",
  "license": "ISC",
  "dependencies": {
    "@financial-times/n-logger": "^6.1.2",
    "aws-sdk": "^2.696.0",
    "body-parser": "^1.19.0",
    "browserify": "^14.5.0",
    "co": "^4.6.0",
    "co-wait": "0.0.0",
    "compression": "^1.7.4",
    "dotenv": "^4.0.0",
    "dynamo-backup-to-s3": "^0.6.1",
    "eslint-plugin-jest": "^23.13.2",
    "event-stream": "^3.3.4",
    "form-data": "^2.5.1",
    "express": "^4.17.1",
    "node-sass": "^4.14.1",
    "s3o-middleware": "^1.8.0",
    "semver": "^5.7.1",
    "snyk": "^1.346.0",
    "ssh2": "^0.8.9"
  },
  "engines": {
    "node": "^8.1.4"
  },
  "devDependencies": {
    "@financial-times/secret-squirrel": "^2.12.0",
    "husky": "^1.3.1",
    "jest": "^26.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "secret-squirrel-commitmsg",
      "pre-commit": "secret-squirrel"
    }
  },
  "snyk": true
}
